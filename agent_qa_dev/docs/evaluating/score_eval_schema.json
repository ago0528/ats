{
  "name": "score_eval",
  "schema": {
    "type": "object",
    "required": [
      "intent_verdict",
      "intent",
      "accuracy",
      "consistency",
      "latencySingle",
      "latencyMulti",
      "stability",
      "reasoning"
    ],
    "properties": {
      "intent_verdict": {
        "type": "string",
        "enum": [
          "PERFECT",
          "GOOD",
          "PARTIAL",
          "WEAK",
          "RELATED_BUT_WRONG",
          "FAILED"
        ]
      },
      "intent": {
        "type": "number",
        "minimum": 0,
        "maximum": 5
      },
      "accuracy": {
        "type": "number",
        "minimum": 0,
        "maximum": 5
      },
      "consistency": {
        "type": [
          "number",
          "null"
        ],
        "minimum": 0,
        "maximum": 5
      },
      "latencySingle": {
        "type": "number",
        "minimum": 0,
        "maximum": 5
      },
      "latencyMulti": {
        "type": "number",
        "minimum": 0,
        "maximum": 5
      },
      "stability": {
        "type": "number",
        "minimum": 0,
        "maximum": 5
      },
      "reasoning": {
        "type": "string"
      }
    },
    "additionalProperties": false
  },
  "strict": true
}
